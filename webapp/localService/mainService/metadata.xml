<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
    xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
    xmlns:sap="http://www.sap.com/Protocols/SAPData">

    <edmx:DataServices m:DataServiceVersion="2.0">
        <Schema Namespace="MyCompanyModelRevisedPK" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">

            <!-- EntityType: Employee -->
            <EntityType Name="Employee" sap:content-version="1">
                <Key>
                    <PropertyRef Name="EmployeeID" />
                </Key>
                <Property Name="EmployeeID" Type="Edm.String" Nullable="false" MaxLength="40" sap:label="Employee ID"/>
                <Property Name="FirstName" Type="Edm.String" Nullable="true" MaxLength="40" sap:label="First Name"/>
                <Property Name="LastName" Type="Edm.String" Nullable="true" MaxLength="40" sap:label="Last Name"/>
                <Property Name="Age" Type="Edm.Int16" Nullable="true" sap:label="Age"/>
                <Property Name="DateHire" Type="Edm.String" Nullable="true" sap:label="Date Hire"/>
                <Property Name="CareerLevel" Type="Edm.String" Nullable="true" MaxLength="2" sap:label="Career Level"/> <!-- FK (conceptually) to CareerList.CareerLevel -->
                <Property Name="CurrentProject" Type="Edm.String" Nullable="true" MaxLength="40" sap:label="Current Project"/> <!-- FK (conceptually) to ProjectList.ProjectName -->
                <!-- Navigation Properties -->
                <NavigationProperty Name="ToSkills" Relationship="MyCompanyModelRevisedPK.EmployeeToSkillLink" FromRole="FromEmployee" ToRole="ToSkill"/>
                <NavigationProperty Name="ToCareerList" Relationship="MyCompanyModelRevisedPK.EmployeeCareerListLink" FromRole="FromEmployee" ToRole="ToCareerList"/>
                <NavigationProperty Name="ToProjectList" Relationship="MyCompanyModelRevisedPK.EmployeeProjectListLink" FromRole="FromEmployee" ToRole="ToProjectList"/>
            </EntityType>

            <!-- EntityType: ProjectList -->
            <EntityType Name="ProjectList" sap:content-version="1">
                <Key>
                    <PropertyRef Name="ProjectId" />
                </Key>
                <Property Name="ProjectId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="Project Id"/>
                <Property Name="ProjectName" Type="Edm.String" Nullable="true" MaxLength="40" sap:label="Project Name"/> <!-- Assumed unique for FK relationship from Employee.CurrentProject -->
                <!-- Navigation Property -->
                <NavigationProperty Name="ToEmployees" Relationship="MyCompanyModelRevisedPK.EmployeeProjectListLink" FromRole="ToProjectList" ToRole="FromEmployee"/>
            </EntityType>

            <!-- EntityType: Skill -->
            <EntityType Name="Skill" sap:content-version="1">
                <Key>
                    <PropertyRef Name="SkillId" /> <!-- PK changed to SkillId -->
                </Key>
                <Property Name="SkillId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="Skill Id"/> <!-- PK, and FK to SkillList.SkillId -->
                <Property Name="EmployeeId" Type="Edm.String" Nullable="true" MaxLength="40" sap:label="Employee Id"/> <!-- FK to Employee.EmployeeID -->
                <Property Name="ProficiencyID" Type="Edm.String" Nullable="true" MaxLength="2" sap:label="Proficiency ID"/> <!-- FK to ProficiencyList.ProficiencyId -->
                <Property Name="SkillName" Type="Edm.String" Nullable="true" MaxLength="20" sap:label="Skill Name"/> <!-- Redundant if SkillId links to SkillList  to be confirmed-->
                <Property Name="ProficiencyLevel" Type="Edm.String" Nullable="true" MaxLength="2" sap:label="Proficiency Level"/> <!-- Redundant if ProficiencyID links to ProficiencyList -->
                <!-- Navigation Properties -->
                <NavigationProperty Name="ToEmployee" Relationship="MyCompanyModelRevisedPK.EmployeeToSkillLink" FromRole="ToSkill" ToRole="FromEmployee"/>
                <NavigationProperty Name="ToSkillList" Relationship="MyCompanyModelRevisedPK.SkillToSkillListLink" FromRole="FromSkill" ToRole="ToSkillList"/>
                <NavigationProperty Name="ToProficiencyList" Relationship="MyCompanyModelRevisedPK.SkillToProficiencyListLink" FromRole="FromSkill" ToRole="ToProficiencyList"/>
            </EntityType>

            <!-- EntityType: ProficiencyList -->
            <EntityType Name="ProficiencyList" sap:content-version="1">
                <Key>
                    <PropertyRef Name="ProficiencyId" />
                </Key>
                <Property Name="ProficiencyId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="Proficiency Id"/>
                <Property Name="ProficiencyLevel" Type="Edm.String" Nullable="true" MaxLength="2" sap:label="Proficiency Level"/>
                <!-- Navigation Property -->
                <NavigationProperty Name="ToSkills" Relationship="MyCompanyModelRevisedPK.SkillToProficiencyListLink" FromRole="ToProficiencyList" ToRole="FromSkill"/>
            </EntityType>

            <!-- EntityType: SkillList -->
            <EntityType Name="SkillList" sap:content-version="1">
                <Key>
                    <PropertyRef Name="SkillId" />
                </Key>
                <Property Name="SkillId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="Skill Id"/>
                <Property Name="SkillName" Type="Edm.String" Nullable="true" MaxLength="20" sap:label="Skill Name"/>
                <!-- Navigation Property -->
                <NavigationProperty Name="ToSkill" Relationship="MyCompanyModelRevisedPK.SkillToSkillListLink" FromRole="ToSkillList" ToRole="FromSkill"/> <!-- Changed from ToSkills to ToSkill due to 1-to-1 -->
            </EntityType>

            <!-- EntityType: CareerList -->
            <EntityType Name="CareerList" sap:content-version="1">
                <Key>
                    <PropertyRef Name="CareerId" />
                </Key>
                <Property Name="CareerId" Type="Edm.String" Nullable="false" MaxLength="10" sap:label="Career Id"/>
                <Property Name="CareerLevel" Type="Edm.String" Nullable="true" MaxLength="2" sap:label="Career Level"/> <!-- Assumed unique for FK relationship from Employee.CareerLevel -->
                <!-- Navigation Property -->
                <NavigationProperty Name="ToEmployees" Relationship="MyCompanyModelRevisedPK.EmployeeCareerListLink" FromRole="ToCareerList" ToRole="FromEmployee"/>
            </EntityType>

            <!-- Associations -->
            <!-- Employee (1) <-> Skill (*) Link: Employee.EmployeeID to Skill.EmployeeId -->
            <Association Name="EmployeeToSkillLink" sap:content-version="1">
                <End Type="MyCompanyModelRevisedPK.Employee" Multiplicity="0..1" Role="FromEmployee"/> <!-- An Employee can have zero or one Skill entry if EmployeeId is unique in Skill, or many if not. Given Skill.SkillId is PK, EmployeeId can repeat.-->
                <End Type="MyCompanyModelRevisedPK.Skill" Multiplicity="*" Role="ToSkill"/>
                <ReferentialConstraint>
                    <Principal Role="FromEmployee">
                        <PropertyRef Name="EmployeeID"/>
                    </Principal>
                    <Dependent Role="ToSkill">
                        <PropertyRef Name="EmployeeId"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>

            <!-- Skill (1) <-> SkillList (1) Link: Skill.SkillId to SkillList.SkillId -->
            <Association Name="SkillToSkillListLink" sap:content-version="1">
                <End Type="MyCompanyModelRevisedPK.SkillList" Multiplicity="1" Role="ToSkillList"/>
                <End Type="MyCompanyModelRevisedPK.Skill" Multiplicity="1" Role="FromSkill"/>
                <ReferentialConstraint>
                    <Principal Role="ToSkillList">
                        <PropertyRef Name="SkillId"/>
                    </Principal>
                    <Dependent Role="FromSkill">
                        <PropertyRef Name="SkillId"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>

            <!-- Skill (*) <-> ProficiencyList (1) Link: Skill.ProficiencyID to ProficiencyList.ProficiencyId -->
            <Association Name="SkillToProficiencyListLink" sap:content-version="1">
                <End Type="MyCompanyModelRevisedPK.ProficiencyList" Multiplicity="0..1" Role="ToProficiencyList"/>
                <End Type="MyCompanyModelRevisedPK.Skill" Multiplicity="*" Role="FromSkill"/>
                <ReferentialConstraint>
                     <Principal Role="ToProficiencyList">
                        <PropertyRef Name="ProficiencyId"/>
                    </Principal>
                    <Dependent Role="FromSkill">
                        <PropertyRef Name="ProficiencyID"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>

            <!-- Employee (*) <-> CareerList (0..1) Link: Employee.CareerLevel to CareerList.CareerLevel -->
            <Association Name="EmployeeCareerListLink" sap:content-version="1">
                <End Type="MyCompanyModelRevisedPK.CareerList" Multiplicity="0..1" Role="ToCareerList"/>
                <End Type="MyCompanyModelRevisedPK.Employee" Multiplicity="*" Role="FromEmployee"/>
                <ReferentialConstraint>
                    <Principal Role="ToCareerList">
                        <PropertyRef Name="CareerLevel"/> <!-- Assumes CareerLevel is unique in CareerList table -->
                    </Principal>
                    <Dependent Role="FromEmployee">
                        <PropertyRef Name="CareerLevel"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>

            <!-- Employee (*) <-> ProjectList (0..1) Link: Employee.CurrentProject to ProjectList.ProjectName -->
            <Association Name="EmployeeProjectListLink" sap:content-version="1">
                <End Type="MyCompanyModelRevisedPK.ProjectList" Multiplicity="0..1" Role="ToProjectList"/>
                <End Type="MyCompanyModelRevisedPK.Employee" Multiplicity="*" Role="FromEmployee"/>
                <ReferentialConstraint>
                    <Principal Role="ToProjectList">
                        <PropertyRef Name="ProjectName"/> <!-- Assumes ProjectName is unique in ProjectList table -->
                    </Principal>
                    <Dependent Role="FromEmployee">
                        <PropertyRef Name="CurrentProject"/>
                    </Dependent>
                </ReferentialConstraint>
            </Association>

            <!-- EntityContainer -->
            <EntityContainer Name="MyCompanyContainerRevisedPK" m:IsDefaultEntityContainer="true" sap:supported-formats="json,xml">
                <EntitySet Name="Employees" EntityType="MyCompanyModelRevisedPK.Employee" sap:content-version="1"/>
                <EntitySet Name="ProjectLists" EntityType="MyCompanyModelRevisedPK.ProjectList" sap:content-version="1"/>
                <EntitySet Name="Skills" EntityType="MyCompanyModelRevisedPK.Skill" sap:content-version="1"/>
                <EntitySet Name="ProficiencyLists" EntityType="MyCompanyModelRevisedPK.ProficiencyList" sap:content-version="1"/>
                <EntitySet Name="SkillLists" EntityType="MyCompanyModelRevisedPK.SkillList" sap:content-version="1"/>
                <EntitySet Name="CareerLists" EntityType="MyCompanyModelRevisedPK.CareerList" sap:content-version="1"/>

                <AssociationSet Name="EmployeeToSkillLinkSet" Association="MyCompanyModelRevisedPK.EmployeeToSkillLink" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1">
                    <End EntitySet="Employees" Role="FromEmployee"/>
                    <End EntitySet="Skills" Role="ToSkill"/>
                </AssociationSet>
                <AssociationSet Name="SkillToSkillListLinkSet" Association="MyCompanyModelRevisedPK.SkillToSkillListLink" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1">
                    <End EntitySet="SkillLists" Role="ToSkillList"/>
                    <End EntitySet="Skills" Role="FromSkill"/>
                </AssociationSet>
                <AssociationSet Name="SkillToProficiencyListLinkSet" Association="MyCompanyModelRevisedPK.SkillToProficiencyListLink" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1">
                    <End EntitySet="ProficiencyLists" Role="ToProficiencyList"/>
                    <End EntitySet="Skills" Role="FromSkill"/>
                </AssociationSet>
                <AssociationSet Name="EmployeeCareerListLinkSet" Association="MyCompanyModelRevisedPK.EmployeeCareerListLink" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1">
                    <End EntitySet="CareerLists" Role="ToCareerList"/>
                    <End EntitySet="Employees" Role="FromEmployee"/>
                </AssociationSet>
                <AssociationSet Name="EmployeeProjectListLinkSet" Association="MyCompanyModelRevisedPK.EmployeeProjectListLink" sap:creatable="false" sap:updatable="false" sap:deletable="false" sap:content-version="1">
                    <End EntitySet="ProjectLists" Role="ToProjectList"/>
                    <End EntitySet="Employees" Role="FromEmployee"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>